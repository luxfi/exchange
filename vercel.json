{
  "rewrites": [
    { "source": "/api/.*",  "destination": "/api/index.js" },
    { "source": "/beta/.*", "destination": "/api/beta.js" },
    { "source": "/cmc/.*",  "destination": "/api/cmc.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' https://www.google-analytics.com https://www.googletagmanager.com 'unsafe-inline';"
        }
      ]
    }
  ]
}
