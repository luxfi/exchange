#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules/wxt/bin/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules/wxt/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules/wxt/bin/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules/wxt/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/wxt@0.20.13_@types+node@25.0.3_jiti@2.6.1_lightningcss@1.27.0_rollup@4.54.0_terser@5.44.1_yaml@2.8.2/node_modules:/Users/z/work/lux/exchange/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../wxt/bin/wxt-publish-extension.cjs" "$@"
else
  exec node  "$basedir/../wxt/bin/wxt-publish-extension.cjs" "$@"
fi
