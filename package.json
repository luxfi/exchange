{
  "name": "@luxfi/exchange-monorepo",
  "version": "5.0.0",
  "private": true,
  "description": "Lux Exchange - DEX Monorepo",
  "homepage": "https://lux.exchange/",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "nx run @luxfi/web:dev",
    "build": "nx run-many -t build",
    "start": "nx run @luxfi/web:start",
    "lint": "nx run-many -t lint",
    "typecheck": "nx run-many -t typecheck",
    "test": "nx run-many -t test",
    "test:e2e": "nx run @luxfi/web:test:e2e",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules .nx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "nx": "^21.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.15.1"
}
